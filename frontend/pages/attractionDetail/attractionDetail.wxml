<view class="attraction-name">
  {{attractionName}}
</view>

<swiper class="image-carousel" autoplay="true" interval="3000" duration="500">
  <block wx:for="{{images}}" wx:key="*this">
    <swiper-item>
      <image src="{{item}}" class="carousel-image"></image>
    </swiper-item>
  </block>
</swiper>